header(css-module="home.menu.header")
  a(css-module="home.menu.logo" href="index" title="Otoy")
    +svg('logo')
  .container
    div(css-module='home.menu.row')
      a(css-module="home.menu.rndr" href="index" title="Otoy")
        +svg('rndr-mobile')

      a(href='index' title='Otoy' css-module='home.menu.tight')
        +svg('otoy-standalone')

      button(css-module='faqs.menu.toggle' data-menu-trigger)
        +svg('burger', {
          'css-module': 'faqs.menu.burger'
        })
        span.sr-only Toggle menu
      ul(css-module='home.menu.normal-links')
        li: a(href="#about") About
        li: a(href='#intro') Render Token
        li: a(href='#problem') The Problem
        li: a(href='#team') Team & Advisors
        li: a(href='#roadmap') Roadmap

      ul(css-module='home.menu.social')
        li: a(href="#"): +svg('twitter')
        li: a(href="#"): +svg('facebook')
        li: a(href="#"): +svg('telegram')
        li: a(href="#"): +svg('reddit')
        li: a(href="#"): +svg('medium')

      a(href="#" title="Launch Rocket Chat" css-module="home.menu.cta")
        +svg('rocket')
        span Launch Rocket Chat
